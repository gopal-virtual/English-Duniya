<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.zayamobile694033" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>English Duniya</name>
  <description>
        High quality learning, anywhere.
    </description>
  <author email="gopal@zaya.in" href="http://www.zaya.in/">
      Gopal Ojha
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="CrosswalkAnimatable" value="true"/>
  <preference name="orientation" value="portrait"/>
  <preference name="webviewbounce" value="true"/>
  <preference name="UIWebViewBounce" value="true"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="13"/>
  <preference name="android-targetSdkVersion" value="23"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="AutoHideSplashScreen" value="false"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <preference name="SplashScreenDelay" value="12000"/>
  <preference name="xwalkVersion" value="19+"/>
  <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect"/>
  <preference name="xwalkMode" value="embedded"/>
  <preference name="xwalkMultipleApk" value="true"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <feature name="SmsInboxPlugin">
    <param name="android-package" value="org.apache.cordova.plugin.SmsInboxPlugin"/>
  </feature>
  <platform name="android">
    <splash density="port-ldpi" src="resources/android/drawable-ldpi/screen.png"/>
    <splash density="port-mdpi" src="resources/android/drawable-mdpi/screen.png"/>
    <splash density="port-xhdpi" src="resources/android/drawable-xhdpi/screen.png"/>
    <splash density="port-xxhdpi" src="resources/android/drawable-xxhdpi/screen.png"/>
    <splash density="port-xxxhdpi" src="resources/android/drawable-xxxhdpi/screen.png"/>
    <icon density="ldpi" src="resources/android/drawable-ldpi/icon.png"/>
    <icon density="mdpi" src="resources/android/drawable-mdpi/icon.png"/>
    <icon density="xhdpi" src="resources/android/drawable-xhdpi/icon.png"/>
    <icon density="xxhdpi" src="resources/android/drawable-xxhdpi/icon.png"/>
    <icon density="xxxhdpi" src="resources/android/drawable-xxxhdpi/icon.png"/>
  </platform>
  <plugin name="cordova-plugin-file" spec="~4.1.1"/>
  <plugin name="cordova-plugin-file-transfer" spec="~1.5.1"/>
  <plugin name="cordova-plugin-nativeaudio" spec="~3.0.7"/>
  <plugin name="cordova-plugin-network-information" spec="~1.2.1"/>
  <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2"/>
  <plugin name="cordova-plugin-compat" spec="~1.0.0"/>
  <plugin name="cordova-plugin-crosswalk-webview" spec="~1.8.0"/>
</widget>
