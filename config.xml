        <splash density="port-xxhdpi" src="resources/android/drawable-xxxhdpi/screen.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/screen.png" />
        <icon density="ldpi" src="resources/android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="resources/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" src="resources/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" src="resources/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" src="resources/android/drawable-xxdpi/icon.png" />
        <icon density="xxxhdpi" src="resources/android/drawable-xxxdpi/icon.png" />
    </platform>
    <platform name="ios">
        <splash height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="resources/ios/splash/De fault-667h.png" width="750" />
        <splash height="2208" src="resources/ios/splash/Default-736h.png" width="1242" />
        <splash height="2048" src="resources/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
