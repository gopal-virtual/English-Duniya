<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.zayamobile694033" version="0.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>English Duniya</name>
    <description>
        High quality learning, anywhere.
    </description>
    <author email="gopal@zaya.in" href="http://www.zaya.in/">
      Gopal Ojha
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <preference name="webviewbounce" value="true" />
    <preference name="UIWebViewBounce" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="xwalkVersion" value="xwalk_core_library_beta:18+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <feature name="SmsInboxPlugin">
        <param name="android-package" value="org.apache.cordova.plugin.SmsInboxPlugin" />
    </feature>
    <platform name="android">
        <splash density="port-ldpi" src="resources/android/drawable-ldpi/screen.png" />
        <splash density="port-mdpi" src="resources/android/drawable-mdpi/screen.png" />
        <splash density="port-hdpi" src="resources/android/drawable-xhdpi/screen.png" />
